{
  "name": "kosen-map",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "check-types": "tsc --noEmit",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "format": "prettier --write .",
    "prepare": "husky install",
    "test": "jest",
    "test-all": "npx yarn-run-all lint check-types format lint:fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o build",
    "storyshots": "jest --config ./jest.storyshots.config.js",
    "update-storyshots": "jest --config ./jest.storyshots.config.js --updateSnapshot"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.1",
    "@mui/material": "^5.0.2",
    "@mui/styled-engine-sc": "^5.0.0",
    "next": "11.1.2",
    "normalize.css": "^8.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "^5.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@react-theming/storybook-addon": "^1.1.1",
    "@storybook/addon-actions": "^6.3.9",
    "@storybook/addon-docs": "^6.3.9",
    "@storybook/addon-essentials": "^6.3.9",
    "@storybook/addon-links": "^6.3.9",
    "@storybook/addon-storyshots": "^6.3.9",
    "@storybook/react": "^6.3.9",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.1",
    "@types/react": "^17.0.24",
    "@types/storybook__addon-storyshots": "^5.3.1",
    "@types/styled-components": "^5.1.14",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.13.2",
    "chromatic": "^5.10.2",
    "eslint": "^7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-react": "^7.26.0",
    "husky": "^7.0.2",
    "jest": "26.6.3",
    "jest-watch-typeahead": "^0.6.4",
    "lint-staged": "^11.1.2",
    "prettier": "2.4.1",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn format",
      "yarn lint:fix"
    ]
  },
  "eslintIgnore": [
    "/jest.config.js",
    "/jest.storyshots.config.js",
    "/test/__mocks__/fileMock.js",
    "/test/__mocks__/styleMock.js"
  ]
}
